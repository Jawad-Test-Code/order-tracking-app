{
  "version": 2,
  "builds": [
    {
      "src": "web/frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "web/frontend/dist"
      }
    },
    {
      "src": "./web/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/web/frontend/dist"
    },
    {
      "src": "/web/(.*)",
      "dest": "/web"
    }
  ]
}